<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXButton?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" alignment="CENTER" >
    <GridPane alignment="CENTER">
        <Label text="ID Store" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
        <JFXTextField fx:id="txtIdStore" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
        <Label text="Direccion" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
        <JFXComboBox fx:id="cmbAddress" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        <JFXButton text="Agregar" fx:id="btnAdd" GridPane.rowIndex="2" GridPane.columnIndex="0"></JFXButton>
        <JFXButton text="Actualizar" fx:id="btnUpdate" GridPane.rowIndex="2" GridPane.columnIndex="1"></JFXButton>
        <JFXButton text="Cancelar" fx:id="btnCancel" GridPane.rowIndex="2" GridPane.columnIndex="2"></JFXButton>
    </GridPane>

</VBox>
